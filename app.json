{
  "expo": {
    "name": "The Wood Wedding",
    "slug": "the-wood-wedding",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "userInterfaceStyle": "light",
    "scheme": "the-wood-wedding",
    "splash": {
      "image": "./assets/splash_screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff9f0"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jack.wood.thewoodwedding"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIconAdaptive.png",
        "backgroundColor": "#fff9f0"
      },
      "package": "com.jack.wood.thewoodwedding"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-secure-store"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0ec5598d-360c-422f-95a2-dd3bccc62d6d"
      }
    }
  }
}
