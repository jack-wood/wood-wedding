{
  "expo": {
    "name": "The Wood Wedding",
    "slug": "the-wood-wedding",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "userInterfaceStyle": "light",
    "scheme": "the-wood-wedding",
    "splash": {
      "image": "./assets/splash_screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff9f0"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "13",
      "supportsTablet": true,
      "bundleIdentifier": "com.jack.wood.thewoodwedding",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "versionCode": 13,
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIconAdaptive.png",
        "backgroundColor": "#fff9f0"
      },
      "package": "com.jack.wood.thewoodwedding",
      "softwareKeyboardLayoutMode": "pan",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@sentry/react-native/expo",
        {
          "organization": "wood-software-ltd",
          "project": "the-wood-wedding",
          // If you are using a self-hosted instance, update the value of the url property
          // to point towards your self-hosted instance. For example, https://self-hosted.example.com/.
          "url": "https://sentry.io/"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0ec5598d-360c-422f-95a2-dd3bccc62d6d"
      }
    }
  }
}
